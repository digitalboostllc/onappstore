{
  "version": 2,
  "buildCommand": "export DATABASE_URL=\"$SUPABASE_POSTGRES_PRISMA_URL\" && export DIRECT_DATABASE_URL=\"$SUPABASE_POSTGRES_URL_NON_POOLING\" && prisma generate && prisma db push --accept-data-loss && next build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "env": {
    "NODE_VERSION": "20",
    "NEXT_TELEMETRY_DISABLED": "1",
    "NEXT_PUBLIC_SUPABASE_URL": "@supabase_next_public_supabase_url",
    "NEXT_PUBLIC_SUPABASE_ANON_KEY": "@supabase_next_public_supabase_anon_key",
    "SUPABASE_SERVICE_ROLE_KEY": "@supabase_supabase_service_role_key",
    "SUPABASE_JWT_SECRET": "@supabase_supabase_jwt_secret",
    "SUPABASE_POSTGRES_URL": "@supabase_postgres_url",
    "SUPABASE_POSTGRES_PRISMA_URL": "@supabase_postgres_prisma_url",
    "SUPABASE_POSTGRES_URL_NON_POOLING": "@supabase_postgres_url_non_pooling",
    "NEXTAUTH_SECRET": "qpglHgESc6Ogwn3ij1sjgw0omxPgbK1Ttola/ACHF2E=",
    "NEXTAUTH_URL": "https://onappstore.vercel.app"
  },
  "build": {
    "env": {
      "NODE_VERSION": "20",
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  },
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "regions": ["iad1"]
} 