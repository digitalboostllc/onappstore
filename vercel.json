{
  "version": 2,
  "buildCommand": "prisma generate && next build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "build": {
    "env": {
      "NODE_VERSION": "20",
      "NEXT_TELEMETRY_DISABLED": "1",
      "DATABASE_URL": "@supabase_postgres_prisma_url",
      "DIRECT_DATABASE_URL": "@supabase_postgres_url_non_pooling",
      "CACHE_BUST": "1"
    }
  },
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "regions": ["iad1"]
} 